{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "scripts/background.js"
    },
    "options_page": "view/options.html",
    "permissions": ["storage","activeTab", "scripting"],
    "action": {
        "default_popup": "view/popup.html",
        "default_icon": {
          "16": "resource/images/get_started16.png",
          "32": "resource/images/get_started32.png",
          "48": "resource/images/get_started48.png",
          "128": "resource/images/get_started128.png"
        }
    },
    "icons": {
        "16":  "resource/images/get_started16.png",
        "32":  "resource/images/get_started32.png",
        "48":  "resource/images/get_started48.png",
        "128": "resource/images/get_started128.png"
    }
}